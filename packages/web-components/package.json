{
  "name": "@department-of-veterans-affairs/web-components",
  "version": "21.2.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm-es5/index.js",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/components/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/component-library/component-library.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    },
    "./react-bindings": {
      "types": "./react-bindings/components.d.ts",
      "import": "./react-bindings/components.js",
      "require": "./react-bindings/components.js",
      "default": "./react-bindings/components.js"
    },
    "./react-bindings/components": {
      "types": "./react-bindings/components.d.ts",
      "import": "./react-bindings/components.js",
      "require": "./react-bindings/components.js",
      "default": "./react-bindings/components.js"
    },
    "./react-bindings/components.server": {
      "types": "./react-bindings/components.server.d.ts",
      "import": "./react-bindings/components.server.js",
      "require": "./react-bindings/components.server.js",
      "default": "./react-bindings/components.server.js"
    },
    "./hydrate": {
      "types": "./hydrate/index.d.ts",
      "import": "./hydrate/index.mjs",
      "require": "./hydrate/index.js",
      "default": "./hydrate/index.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs.js",
      "default": "./loader/index.cjs.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist/",
    "loader/",
    "component-docs.json",
    "react-bindings/"
  ],
  "scripts": {
    "build": "stencil build --docs-json component-docs.json",
    "build-bindings": "tsc -p tsconfig.bindings.json",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "watch:stencil": "stencil build --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "typecheck": "tsc --noEmit",
    "typecheck-bindings": "tsc -p tsconfig.bindings.json --noEmit",
    "generate": "stencil generate",
    "serve": "stencil build --dev --watch --serve"
  },
  "dependencies": {
    "@stencil/core": "4.20.0",
    "@stencil/react-output-target": "^1.2.0",
    "aria-hidden": "^1.1.3",
    "body-scroll-lock": "^4.0.0-beta.0",
    "chromatic": "^11.0.4",
    "classnames": "^2.3.1",
    "intersection-observer": "^0.12.0",
    "libphonenumber-js": "^1.12.7"
  },
  "license": "MIT",
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.0",
    "@babel/core": "^7.12.13",
    "@department-of-veterans-affairs/css-library": "^0.26.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/sass": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^22.5.0",
    "@types/react-dom": "19.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@uswds/uswds": "^3.9.0",
    "animate.css": "^4.1.1",
    "babel-loader": "^8.2.2",
    "eslint": "^8.19.0",
    "eslint-plugin-i18next": "^6.0.0-4",
    "jest": "^29",
    "jest-cli": "^29",
    "jest-transformer-svg": "^2.0.1",
    "lit-html": "^1.3.0",
    "postcss-url": "^10.1.1",
    "puppeteer": "21.0.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "i18next": "*",
    "i18next-browser-languagedetector": "*"
  }
}
