{
  "name": "@department-of-veterans-affairs/web-components",
  "version": "15.1.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm-es5/index.js",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/components/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/component-library/component-library.esm.js",
  "files": [
    "dist/",
    "loader/",
    "component-docs.json",
    "react-bindings/"
  ],
  "scripts": {
    "build": "stencil build --docs-json component-docs.json",
    "build-bindings": "tsc -p tsconfig.bindings.json",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "watch:stencil": "stencil build --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "serve": "stencil build --dev --watch --serve"
  },
  "dependencies": {
    "@department-of-veterans-affairs/css-library": "^0.12.2",
    "@stencil/core": "4.20.0",
    "aria-hidden": "^1.1.3",
    "body-scroll-lock": "^4.0.0-beta.0",
    "chromatic": "^11.0.4",
    "classnames": "^2.3.1",
    "intersection-observer": "^0.12.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@axe-core/puppeteer": "^4.4.0",
    "@babel/core": "^7.12.13",
    "@department-of-veterans-affairs/formation": "^11.0.6",
    "@stencil/postcss": "^2.0.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/sass": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^22.5.0",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@uswds/uswds": "^3.8.1",
    "animate.css": "^4.1.1",
    "babel-loader": "^8.2.2",
    "eslint": "^8.19.0",
    "eslint-plugin-i18next": "^6.0.0-4",
    "jest": "^29",
    "jest-cli": "^29",
    "jest-transformer-svg": "^2.0.1",
    "lit-html": "^1.3.0",
    "postcss-url": "^10.1.1",
    "puppeteer": "^21.0.0",
    "react-dom": "^18.3.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "@department-of-veterans-affairs/formation": "^11.0.6",
    "i18next": "*",
    "i18next-browser-languagedetector": "*"
  }
}
