{
  "name": "@department-of-veterans-affairs/web-components",
  "type": "module",
  "version": "23.0.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm-es5/index.js",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/components/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/component-library/component-library.esm.js",
  "files": [
    "dist/",
    "loader/",
    "component-docs.json",
    "react-bindings/"
  ],
  "scripts": {
    "build": "BROWSERSLIST_IGNORE_OLD_DATA=1 stencil build --docs-json component-docs.json",
    "copy-bindings": "node ./scripts/copy-bindings.cjs",
    "build-bindings": "tsc -p tsconfig.bindings.json",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "watch:stencil": "BROWSERSLIST_IGNORE_OLD_DATA=1 stencil build --watch",
    "test": "BROWSERSLIST_IGNORE_OLD_DATA=1 stencil test --spec --e2e --runInBand",
    "test.watch": "BROWSERSLIST_IGNORE_OLD_DATA=1 stencil test --spec --e2e --watchAll --runInBand",
    "generate": "stencil generate",
    "serve": "stencil build --dev --watch --serve"
  },
  "dependencies": {
    "@stencil/core": "4.25.0",
    "aria-hidden": "1.1.3",
    "body-scroll-lock": "4.0.0-beta.0",
    "chromatic": "11.0.4",
    "classnames": "2.3.1",
    "intersection-observer": "0.12.0",
    "libphonenumber-js": "1.12.7"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/department-of-veterans-affairs/component-library.git"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "4.10.0",
    "@babel/core": "7.12.13",
    "@department-of-veterans-affairs/css-library": "0.27.0",
    "@stencil/postcss": "2.1.0",
    "@stencil/react-output-target": "0.5.3",
    "@stencil/sass": "3.2.3",
    "@types/jest": "26.0.24",
    "@types/node": "22.5.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "@uswds/uswds": "3.9.0",
    "animate.css": "4.1.1",
    "babel-loader": "8.2.2",
    "eslint": "9.26.0",
    "eslint-plugin-i18next": "6.0.0-4",
    "jest": "29",
    "jest-cli": "29",
    "jest-transformer-svg": "2.0.1",
    "lit-html": "1.4.1",
    "postcss-url": "10.1.1",
    "puppeteer": "24.37.2",
    "react-dom": "17.0.2",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "i18next": "25.6.0",
    "i18next-browser-languagedetector": "8.2.0",
    "react": "17.0.2"
  }
}
