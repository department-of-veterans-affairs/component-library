{
  "name": "web-components",
  "version": "0.10.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/component-library/component-library.esm.js",
  "files": [
    "dist/",
    "loader/",
    "component-docs.json",
    "react-bindings/"
  ],
  "scripts": {
    "build": "stencil build --docs-json component-docs.json",
    "build-bindings": "tsc -p tsconfig.bindings.json",
    "start": "concurrently -n storybook,stencil -c \"bgMagenta.bold,bgBlue.bold\" \"npm:storybook\" \"npm:watch:stencil\"",
    "watch:stencil": "stencil build --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "serve": "stencil build --dev --watch --serve",
    "build-storybook": "build-storybook",
    "prepare": "npm run build && npm run build-bindings"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "@stencil/postcss": "^2.0.0",
    "postcss-url": "^10.1.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@axe-core/puppeteer": "^4.1.1",
    "@babel/core": "^7.12.13",
    "@department-of-veterans-affairs/formation": "^6.14.0",
    "@stencil/react-output-target": "^0.0.9",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/web-components": "^6.1.17",
    "@types/jest": "^26.0.20",
    "@types/puppeteer": "^5.4.3",
    "@types/react-dom": "16.0.9",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^1.3.0",
    "puppeteer": "5.4.1",
    "react-dom": "^16.7.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@department-of-veterans-affairs/formation": "^6.14.0"
  }
}
